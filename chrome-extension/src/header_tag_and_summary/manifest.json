{
  "manifest_version": 3,

  "name": "Modify BBC article HTML",
  "version": "0.1.0",
  "description": "Modifies BBC article HTML",
 
  "web_accessible_resources": [{
    "resources": ["images/*"],
    "matches": ["https://www.bbc.co.uk/news/articles/*"]
  }],

  "content_scripts": [{
    "js": ["script.js"],
    "css": ["styles.css"],
    "matches": ["https://www.bbc.co.uk/news/articles/*"]
  }]
}